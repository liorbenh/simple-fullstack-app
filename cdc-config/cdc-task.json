{
  "changefeed_id": "tidb-to-kafka",
  "sink_uri": "kafka://kafka:29092/tidb-cdc-changes?protocol=open-protocol",
  "rules": ["sre_test.*"],
  "config": {
    "case-sensitive": false,
    "enable-old-value": true,
    "force-replicate": false,
    "filter": {
      "rules": ["sre_test.*"]
    },
    "sink": {
      "protocol": "open-protocol"
    }
  }
}
